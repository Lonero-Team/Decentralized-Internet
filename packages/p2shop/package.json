{
  "name": "p2shop",
  "version": "1.0.4",
  "description": "E-Com Plus's cart library w/ Decentralized Internet backend",
  "module": "src/index.js",
  "browser": "dist/ecom-cart.min.js",
  "main": "dist/ecom-cart.node.js",
  "jsdelivr": "dist/ecom-cart.var.min.js",
  "unpkg": "dist/ecom-cart.var.min.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "serve": "webpack-dev-server",
    "doc": "rm -rf ./docs/*; jsdoc ./src -r ./README.md -d ./docs -t ./node_modules/docdash",
    "release": "rm -rf ./dist; npm ddp && npm run build && standard-version",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "ecommerce",
    "storefront",
    "shopping",
    "decentralized",
    "blockchain",
    "p2p",
    "digital",
    "cart"
  ],
  "author": "The Lonero Foundation",
  "license": "MIT",
  "peerDependencies": {
    "@ecomplus/utils": "^1.2.0"
  },
  "optionalDependencies": {
    "core-js": "3.x"
  },
  "dependencies": {
    "eventemitter3": "^5.0.0",
    "decentralized-internet": "^5.0.0",
    "@snyk/protect": "latest"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@ecomplus/utils": "1.4.1",
    "babel-loader": "10.0.0",
    "core-js": "3.45.1",
    "docdash": "2.0.2",
    "husky": "9.1.7",
    "jsdoc": "4.0.4",
    "standard": "17.1.2",
    "standard-version": "9.5.0",
    "webpack": "5.101.3",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "> 0.2%"
  ],
  "snyk": true
}
